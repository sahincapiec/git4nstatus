    "start-watching": "env-cmd --no-override -f ./config/dev.env nodemon dist/app.js -e js",
    "start-watching-pnpm": "pnpx env-cmd --no-override -f ./config/dev.env nodemon dist/app.js -e js",
    "test": "npx jest",
    "test-pnpm": "pnpx jest",
    "test-watching": "npx jest --watch",
    "test-watching-pnpm": "pnpx jest --watch"
